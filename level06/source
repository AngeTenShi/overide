#include <stdio.h>

int auth(char *login, int serialNumber)
{
    int i;
    int sum = 0;
    int length = strnlen(login, 32);
    login[strcspn(login, "\n")] = '\0';
    if (length <= 5)
        return 1;
    if (ptrace(0, 0, 1, 0) == -1)
    {
        puts("\x1B[32m.---------------------------.");
        puts("\x1B[31m| !! TAMPERING DETECTED !!  |");
        puts("\x1B[32m'---------------------------'");
        return 1;
    }
    else
    {
        int key = (login[3] ^ 4919) + 6221293;

        for (i = 0; i < length; ++i)
        {
            if (login[i] <= 31)
                return 1;
            sum += (key ^ (unsigned int)login[i]) % 0x539;
        }

        return serialNumber != sum;
    }
}

int main(int argc, const char **argv, const char **envp)
{
    int serialNumber;
    char login[32];

    puts("***********************************");
    puts("*\t\tlevel06\t\t  *");
    puts("***********************************");
    printf("-> Enter Login: ");
    fgets(login, sizeof(login), stdin);
    puts("***********************************");
    puts("***** NEW ACCOUNT DETECTED ********");
    puts("***********************************");
    printf("-> Enter Serial: ");
    scanf("%d", &serialNumber);
    if (auth(login, serialNumber))
        return 1;
    puts("Authenticated!");
    system("/bin/sh");
    return 0;
}